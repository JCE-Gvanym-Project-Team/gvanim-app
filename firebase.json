{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
          "source": "**",
          "destination": "/index.html"
      }
  ],
  "mime": {
    "type": "text/javascript",
    "source": "**/*.js"
  }
  },
  "storage": {
    "rules": "storage.rules"
  }
}
